# ===========================================
# WP-STUDIO ENVIRONMENT CONFIGURATION TEMPLATE
# ===========================================
# Copy this to .env and fill in your values

# Database Configuration
DB_NAME=wp_studio
DB_USER=wpuser
DB_PASSWORD=your_secure_password_here
DB_ROOT_PASSWORD=your_root_password_here

# WordPress Configuration
WP_ENV=development
WP_HOME=http://localhost:8000
WP_SITEURL=http://localhost:8000

# Admin User
WP_ADMIN_USER=admin
WP_ADMIN_PASSWORD=change_me
WP_ADMIN_EMAIL=your_email@example.com

# Generate security keys at: https://api.wordpress.org/secret-key/1.1/salt/
AUTH_KEY='generate-your-own-key'
SECURE_AUTH_KEY='generate-your-own-key'
LOGGED_IN_KEY='generate-your-own-key'
NONCE_KEY='generate-your-own-key'
AUTH_SALT='generate-your-own-key'
SECURE_AUTH_SALT='generate-your-own-key'
LOGGED_IN_SALT='generate-your-own-key'
NONCE_SALT='generate-your-own-key'